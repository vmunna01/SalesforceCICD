<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>SDOC__Task_Auto_Create_Default__c</fullName>
    <deprecated>false</deprecated>
    <description>Controls auto-task creation defaults</description>
    <externalId>false</externalId>
    <inlineHelpText>Use this field to control if you want a task to be auto-created, required, or if user needs to opt-in to create a task.</inlineHelpText>
    <label>Task Auto Create Default</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Picklist</type>
    <valueSet>
        <restricted>true</restricted>
        <valueSetDefinition>
            <sorted>false</sorted>
            <value>
                <fullName>Checked1</fullName>
                <default>false</default>
                <label>Checked, user can Uncheck to prevent Task Creation</label>
            </value>
            <value>
                <fullName>Checked2</fullName>
                <default>false</default>
                <label>Checked, User cannot uncheck</label>
            </value>
            <value>
                <fullName>Unchecked</fullName>
                <default>false</default>
                <label>Unchecked, user can Check to Auto-Create a task</label>
            </value>
        </valueSetDefinition>
    </valueSet>
</CustomField>
